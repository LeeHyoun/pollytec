<head>
    <script>
        // 변수를 생성합니다.
        var 함수 = function () {
            var output = prompt('숫자를 입력해주세요.', '숫자');
            alert(output);
        };
        
        function 함수2() {
            var output2 = prompt('숫자를 입력해주세요.', '숫자');
            alert(output2);
        };
        함수();
        함수2(); 
        // 출력합니다.
        alert(함수);
        //alert(함수2());
    </script>
